<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="550.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SelectionSortController">
   <children>
      <Pane fx:id="bPane" prefHeight="550.0" prefWidth="900.0">
         <children>
            <HBox alignment="CENTER" layoutX="181.0" layoutY="456.0" prefHeight="80.0" prefWidth="539.0" spacing="30.0">
               <children>
                  <Button fx:id="generate" mnemonicParsing="false" onAction="#generateArray" text="Generate" />
                  <Button fx:id="back" mnemonicParsing="false" onAction="#back" text="Back" />
                  <Button fx:id="startSort" mnemonicParsing="false" onAction="#startSorting" text="Begin Sorting" />
               </children>
            </HBox>
            <VBox alignment="CENTER" layoutX="236.0" layoutY="14.0" prefHeight="103.0" prefWidth="428.0">
               <children>
                  <Label text="Selection Sort">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
